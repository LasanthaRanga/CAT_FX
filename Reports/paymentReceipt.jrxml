	<parameter name="paymentId" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT cat.payment.`year`,
	cat.payment.`month`,
	cat.payment.application_no,
	cat.payment.payment_date,
	cat.payment.receipt_no,
	cat.payment.tax_amount,
	cat.payment.vat,
	cat.payment.nbt,
	cat.payment.spamp,
	cat.payment.totale_amount,
	cat.vort.vote_name,
	cat.application.allocation,
	cat.application.application_date,
	cat.customer.full_name,
	cat.application.trade_name,
	cat.trade_type.type_name,
	cat.trade_license.licen_no,
	cat.trade_nature.nature,
	cat.sub_nature.sub_nature
FROM cat.trade_license
	INNER JOIN cat.payment ON 
	 cat.trade_license.`Payment_idPayment` = cat.payment.`idPayment` 
	INNER JOIN cat.vort ON 
	 cat.payment.`Vort_idVort` = cat.vort.`idVort` 
	INNER JOIN cat.application ON 
	 cat.application.`idApplication` = cat.payment.application_no 
	 AND cat.trade_license.`Application_idApplication` = cat.application.`idApplication` 
	INNER JOIN cat.customer ON 
	 cat.application.`Customer_idCustomer` = cat.customer.`idCustomer` 
	INNER JOIN cat.trade_type ON 
	 cat.application.`Trade_Type_idTrade_Type` = cat.trade_type.`idTrade_Type` 
	INNER JOIN cat.trade_nature ON 
	 cat.application.`Trade_Nature_idTrade_Nature` = cat.trade_nature.`idTrade_Nature` 
	INNER JOIN cat.sub_nature ON 
	 cat.application.`Sub_Nature_idSub_Nature` = cat.sub_nature.`idSub_Nature` 
WHERE 
	 cat.payment.`idPayment` = $P{paymentId}]]>
	</queryString>
