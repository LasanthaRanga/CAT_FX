<AnchorPane id="AnchorPane" fx:id="anchor_main" onMouseDragged="#mouseDragged" onMousePressed="#mousePressed" onMouseReleased="#mouseReleased" prefHeight="345.0" prefWidth="410.0" style="-fx-background-color: rgb(2,136,209,0.9);" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
