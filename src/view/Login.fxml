      </Label>
      <JFXTextField fx:id="txt_uname" focusColor="WHITE" labelFloat="true" layoutX="75.0" layoutY="113.0" onMousePressed="#mousePressed" prefHeight="33.0" prefWidth="272.0" promptText="User Name" unFocusColor="#4900f4">
         <font>
            <Font size="16.0" />
         </font>
      </JFXTextField>
      <JFXPasswordField fx:id="txt_pass" focusColor="WHITE" labelFloat="true" layoutX="75.0" layoutY="180.0" onKeyReleased="#enterLog" prefHeight="33.0" prefWidth="272.0" promptText="Password" unFocusColor="#4900f4">
